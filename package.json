{
  "name": "cashlint-backend",
  "version": "1.0.0",
  "type": "module",
  "description": "Cashlint is a simple bookkeeping tool designed around accrual accounting principles. It supports invoices, bills, loans, and owner equity tracking in a clean workflow",
  "scripts": {
    "test:unit": "vitest --watch --project unit",
    "test:integration": "vitest run --project integration",
    "test:all": "vitest run --project unit && pnpm test:integration",
    "dev": "tsx src/api/server.ts",
    "prisma:migrate": "prisma migrate dev",
    "prisma:generate": "prisma generate"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.24.0",
  "dependencies": {
    "@prisma/adapter-mariadb": "^7.0.1",
    "@prisma/client": "^7.0.1",
    "cookie-parser": "^1.4.7",
    "express": "^5.1.0",
    "ramda": "^0.32.0",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "@types/ramda": "^0.31.1",
    "@types/supertest": "^6.0.3",
    "prisma": "^7.0.1",
    "supertest": "^7.1.4",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.21.0",
    "vitest": "^4.0.14"
  },
  "_moduleAliases": {
    "@": "src"
  }
}
